---
layout: null
---

{
    "plugins": [{% for plugin in site.plugins limit:10 %}
    {
        "title":       "{{ plugin.title }}",
        "description": "{{ plugin.description | strip_newlines }}",
        "author":      "{{ plugin.author }}",
        "repository":  "{{ plugin.repository }}"
    }{% unless forloop.last %},{% endunless %}{% endfor %}
    ]
}
